                <property name="factor" value="100.0" type="Double" description="factor to measurement value"/>
            </Measurement>
            <Measurement>
                <name>temperature extern</name>
                <symbol>T</symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
