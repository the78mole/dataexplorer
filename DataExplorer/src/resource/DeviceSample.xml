ase>	<DataBlock>		<format size="25" formatType="BINARY" inputType="FILE_IO"/>		<checkSum type="ADD" format="BINARY"/>		<timeUnit>sec</timeUnit>		<separator>semicolon</separator>		<leader>$</leader>		<trailer>0D0A</trailer>	</DataBlock>	<State>		<property type="Integer" description="charge activity" value="1" name="state_charge"/>		<property type="Integer" description="discharge activity" value="2" name="state_discharge"/>	</State>	<Channels>		<Channel type="TYPE_OUTLET" name="type_outlet">			<Measurement>				<name>voltage</name>				<symbol>U</symbol>				<unit>V</unit>				<active>true</active>				<statistics sigma="false" avg="false" max="true" min="true"/>			</Measurement>			<Measurement>				<name>current</name>				<symbol>I</symbol>				<unit>A</unit>				<active>true</active>				<statistics sigma="true" avg="true" max="true" min="true"/>			</Measurement>			<Measurement>				<name>capacity</name>				<symbol>C</symbol>				<unit>mAh</unit>				<active>true</active>				<statistics sigma="false" avg="false" max="true" min="true"/>			</Measurement>			<Measurement>				<name>power</name>				<symbol>P</symbol>				<unit>W</unit>				<statistics sigma="false" avg="false" max="true" min="true"/>			</Measurement>			<Measurement>				<name>energy</name>				<symbol>E</symbol>				<unit>Wh</unit>				<statistics sigma="false" avg="false" max="true" min="true"/>			</Measurement>		</Channel>	</Channels>	<Desktop>		<property value="true" name="table_tab"/>		<property value="true" name="digital_tab"/>		<property value="true" name="analog_tab"/>		<property value="false" name="voltage_per_cell_tab"/>	</Desktop></DeviceProperties>