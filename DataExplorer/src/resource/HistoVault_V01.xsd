:element name="vaultDeviceKey" type="xs:string" />			<xs:element name="vaultDeviceName" type="xs:string" />			<xs:element name="vaultChannelNumber" type="xs:int" />			<xs:element name="vaultObjectKey" type="xs:string" />			<xs:element name="vaultSamplingTimespan_ms" type="xs:long" />			<xs:element name="logFilePath" type="xs:string">				<xs:annotation>					<xs:documentation>path to the origin file itself, this is not a link file path					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="logFileLastModified" type="xs:long" />			<xs:element name="logFileLength" type="xs:long" />			<xs:element name="logObjectDirectory" type="xs:string" />			<xs:element name="logFileVersion" type="xs:int" />			<xs:element name="logRecordSetSize" type="xs:int" />			<xs:element name="logRecordSetOrdinal" type="xs:int" />			<xs:element name="logRecordsetBaseName" type="xs:string" />			<xs:element name="logDeviceName" type="xs:string" />			<xs:element name="logChannelNumber" type="xs:int" />			<xs:element name="logObjectKey" type="xs:string" />			<xs:element name="logStartTimestamp_ms" type="xs:long" />			<xs:element name="measurements" type="compartmentsType" />			<xs:element name="settlements" type="compartmentsType" />			<xs:element name="scores" type="pointsType" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="compartmentsType">		<xs:annotation>			<xs:documentation>holds either measurements or settlements			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="compartment" type="compartmentType" minOccurs="0" maxOccurs="unbounded" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="compartmentType">		<xs:sequence>			<xs:element name="trails" type="pointsType" />		</xs:sequence>		<xs:attribute type="xs:int" name="id" use="required" />		<xs:attribute type="xs:string" name="text" use="required">			<xs:annotation>				<xs:documentation>Measurement / settlement name for human readers only. Does not require					localization.				</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attribute type="data_types" name="dataType" use="required" />	</xs:complexType>	<xs:complexType name="pointsType">		<xs:annotation>			<xs:documentation>holds points for scores or trails			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="point" type="pointType" maxOccurs="unbounded" minOccurs="0" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="pointType">		<xs:simpleContent>			<xs:extension base="xs:int">				<xs:attribute type="xs:int" name="id" use="required" />				<xs:attribute type="xs:string" name="name" use="required">					<xs:annotation>						<xs:documentation>holds score labels for scores or trail names for measurements / settlements						</xs:documentation>					</xs:annotation>				</xs:attribute>			</xs:extension>		</xs:simpleContent>	</xs:complexType>	<xs:simpleType name="data_types">		<xs:restriction base="xs:string">			<xs:enumeration value="default">				<xs:annotation>					<xs:documentation>all normal measurement values which do not require special handling					</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="gps_latitude">				<xs:annotation>					<xs:documentation>GPS geo-coordinate require at least 6 decimal digits [°]					</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="gps_longitude">				<xs:annotation>					<xs:documentation>GPS geo-coordinate require at least 6 decimal digits [°]					</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="gps_altitude">				<xs:annotation>					<xs:documentation>GPS or absolute altitude required in some case for GPS related calculations						like speed, distance, ...					</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="gps_azimuth">				<xs:annotation>					<xs:documentation>GPS azimuth, to be used for live display and positioning of icon if used					</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="speed">				<xs:annotation>					<xs:documentation>speed, to be used for KMZ export with colors of specified velocity					</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="date_time">				<xs:annotation>					<xs:documentation>special data type where no formatting or calculation can be executed, 						just display drops					</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="current">				<xs:annotation>					<xs:documentation>data type to unique identify current type, mainly used for smoothing current						voltage values					</xs:documentation>				</xs:annotation>			</xs:enumeration>			<xs:enumeration value="voltage">				<xs:annotation>					<xs:documentation>data type to unique identify voltage type, to smoothing reflex or pulsing						voltage values					</xs:documentation>				</xs:annotation>			</xs:enumeration>		</xs:restriction>	</xs:simpleType></xs:schema>
