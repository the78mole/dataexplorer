/>			<xs:element name="logFilePath" type="xs:string">				<xs:annotation>					<xs:documentation>path to the origin file itself, this is not a link file path					</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="logFileLastModified" type="xs:long" />			<xs:element name="logObjectDirectory" type="xs:string" />			<xs:element name="logRecordSetNumber" type="xs:int" />			<xs:element name="logRecordsetName" type="xs:string" />			<xs:element name="logDeviceName" type="xs:string" />			<xs:element name="logChannelNumber" type="xs:int" />			<xs:element name="logObjectKey" type="xs:string" />			<xs:element name="logStartTimestamp_ms" type="xs:long" />			<xs:element name="measurements" type="entries" />			<xs:element name="settlements" type="entries" />			<xs:element name="scores" type="entryPoints" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="entries">		<xs:annotation>			<xs:documentation>represents measurements or settlements			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="entryPoints" type="entryPoints"				maxOccurs="unbounded" minOccurs="0" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="entryPoints">		<xs:annotation>			<xs:documentation>holds either trails for measurements and				settlements or score labels for scores			</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="point" type="point" maxOccurs="unbounded"				minOccurs="0" />		</xs:sequence>		<xs:attribute type="xs:string" name="name" use="required" />		<xs:attribute type="xs:int" name="id" use="required" />	</xs:complexType>	<xs:complexType name="point">		<xs:simpleContent>			<xs:extension base="xs:int">				<xs:attribute type="xs:string" name="name" use="required" />				<xs:attribute type="xs:int" name="id" use="required" />			</xs:extension>		</xs:simpleContent>	</xs:complexType></xs:schema>
