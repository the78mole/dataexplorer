        <Channel name="type_general" type="TYPE_CONFIG">
            <Measurement>
                <name>RXSQ</name>
                <symbol></symbol>
                <unit>%</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>voltage_rx</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
