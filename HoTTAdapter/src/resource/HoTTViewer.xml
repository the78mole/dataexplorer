    <TimeBase>
        <name>time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>-1.0</timeStep>
        <UTCdelta>0</UTCdelta>
    </TimeBase>
    <State>
        <property name="state_data_recording" value="01" type="Integer"/>
    </State>
