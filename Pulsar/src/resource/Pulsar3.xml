<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V31.xsd">
    <Device>
        <name>Pulsar3</name>
        <manufacturer>Elprog</manufacturer>
        <manufacturerURL>http://www.elprog.com.pl/english</manufacturerURL>
        <group>charger</group>
        <image>Pulsar3.png</image>
        <usage>false</usage>
    </Device>
    <SerialPort>
        <port>/dev/tty.Bluetooth-Incoming-Port</port>
        <baudeRate>19200</baudeRate>
        <dataBits>DATABITS_8</dataBits>
        <stopBits>STOPBITS_1</stopBits>
        <parity>PARITY_NONE</parity>
        <flowControlMode>FLOWCONTROL_NONE</flowControlMode>
        <isRTS>false</isRTS>
        <isDTR>false</isDTR>
        <TimeOut>
            <ReadTimeOut>5000</ReadTimeOut>
            <ReadStableIndex>8</ReadStableIndex>
            <WriteCharDelayTime>0</WriteCharDelayTime>
            <WriteDelayTime>0</WriteDelayTime>
        </TimeOut>
    </SerialPort>
    <TimeBase>
        <name>time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>5000.0</timeStep>
    </TimeBase>
    <DataBlock>
        <format inputType="SERIAL_IO" formatType="BYTE" size="160"/>
        <format inputType="FILE_IO" formatType="VALUE" size="24"/>
        <timeUnit>sec</timeUnit>
        <separator>comma</separator>
        <leader>#</leader>
        <trailer>0D0A</trailer>
        <preferredDataLocation>/Users/brueg/Documents/DataExplorer/Pulsar3</preferredDataLocation>
        <preferredFileExtention>*.acp</preferredFileExtention>
    </DataBlock>
    <State>
        <property name="state_charge" value="1" type="Integer" description="Ladeoperation"/>
        <property name="state_discharge" value="2" type="Integer" description="Entladeoperation"/>
        <property name="state_monitoring" value="3" type="Integer" description="Beobachten"/>
        <property name="state_dry_time" value="4" type="Integer" description="Warten auf anderen Status"/>
        <property name="state_finish" value="5" type="Integer" description="alle Programme abgelaufen"/>
        <property name="state_error" value="6" type="Integer" description="Fehler"/>
    </State>
