<?xml version="1.0" encoding="UTF-8" standalone="yes"?><DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V41.xsd">	<Device>		<name>Picolario</name>		<manufacturer>Renschler</manufacturer>		<manufacturerURL>https://www.renschler.de</manufacturerURL>		<group>logger</group>		<image>Picolario.jpg</image>		<usage>false</usage>	</Device>	<SerialPort>		<port>-</port>		<baudeRate>38400</baudeRate>		<dataBits>DATABITS_8</dataBits>		<stopBits>STOPBITS_1</stopBits>		<parity>PARITY_NONE</parity>		<flowControlMode>FLOWCONTROL_NONE</flowControlMode>		<isRTS>true</isRTS>		<isDTR>false</isDTR>	</SerialPort>	<TimeBase>		<name>time</name>		<symbol>t</symbol>		<unit>msec</unit>		<timeStep>50</timeStep>	</TimeBase>    <DataBlock>        <format inputType="SERIAL_IO" formatType="BYTE" size="-31"/>		<checkSum type="XOR" format="BINARY"/>    </DataBlock>    <Channels>		<Channel type="TYPE_CONFIG" name="type_thermal">			<Measurement>				<name>voltage</name>				<symbol>U</symbol>				<unit>V</unit>				<active>true</active>				<statistics sigma="true" avg="true" max="true" min="true"/>				<property description="offset to value" type="Double" value="2.5" name="offset"/>				<property description="reduction of value before othe arithmetic" type="Double" value="45" name="reduction"/>				<property description="Factor applied to value" type="Double" value="0.0532" name="factor"/>			</Measurement>			<Measurement>				<name>altitude_relative</name>				<symbol>h</symbol>				<unit>m</unit>				<active>true</active>				<statistics sigma="false" avg="true" max="true" min="true"/>				<property description="Altitude calculation from Feet to Meter  (1852 / 6076)" type="Double" value="0.304806" name="factor"/>				<property description="Do not adapt altitude values" type="Boolean" value="false" name="do_no_adaption"/>				<property description="Subtract first value from all successors" type="Boolean" value="true" name="do_subtract_first"/>				<property description="Subtract last value from all predecessors" type="Boolean" value="false" name="do_subtract_last"/>				<property description="do offset to altitude value" type="Boolean" value="false" name="do_offset_height"/>				<property description="Altitude offset value" type="Double" value="0.0" name="offset"/>			</Measurement>			<Measurement>				<name>climb</name>				<symbol>V</symbol>				<unit>m/s</unit>				<statistics sigma="false" avg="false" max="true" min="true"/>				<property description="Regression interval in seconds" type="Integer" value="10" name="regression_interval_sec"/>				<property description="Regression arithmetic" type="String" value="regression_type_curve" name="regression_type"/>			</Measurement>		</Channel>		<Channel type="TYPE_CONFIG" name="type_motor">			<Measurement>				<name>voltage</name>				<symbol>U</symbol>				<unit>V</unit>				<active>true</active>				<statistics sigma="true" avg="true" max="true" min="true"/>				<property description="offset to value" type="Double" value="2.5" name="offset"/>				<property description="reduction of value before othe arithmetic" type="Double" value="45" name="reduction"/>				<property description="Factor applied to value" type="Double" value="0.0532" name="factor"/>			</Measurement>			<Measurement>				<name>altitude_relative</name>				<symbol>H</symbol>				<unit>m</unit>				<active>true</active>				<statistics countTriggerText="text_trigger_count" sumTriggerText="text_trigger_motor_sum" sumByTriggerRefOrdinal="2" sigma="false" avg="false" max="true" min="true"/>				<property description="Altitude calculation from Feet to Meter  (1852 / 6076)" type="Double" value="0.304806" name="factor"/>				<property description="Do not adapt altitude values" type="Boolean" value="false" name="do_no_adaption"/>				<property description="Subtract first value from all successors" type="Boolean" value="false" name="do_subtract_first"/>				<property description="Subtract last value from all predecessors" type="Boolean" value="true" name="subtract_last"/>				<property description="do offset to altitude value" type="Boolean" value="false" name="do_offset_height"/>				<property description="Altitude offset value" type="Double" value="0.0" name="offset"/>			</Measurement>			<Measurement>				<name>climb</name>				<symbol>V</symbol>				<unit>m/s</unit>				<statistics countTriggerText="text_trigger_count" countByTrigger="true" sigma="true" avg="true" max="true" min="false" sumTriggerTimeText="text_trigger_time_sum">					<trigger comment="comment_trigger_climb_def" minTimeSec="8" isGreater="true" level="1500"/>				</statistics>				<property description="Regression interval in seconds" type="Integer" value="10" name="regression_interval_sec"/>				<property description="Regression arithmetic" type="String" value="regression_type_curve" name="regression_type"/>			</Measurement>		</Channel>	</Channels>	<Desktop>		<property value="false" name="table_tab"/>		<property value="false" name="voltage_per_cell_tab"/>		<property value="false" name="analog_tab"/>		<property value="false" name="digital_tab"/>	</Desktop></DeviceProperties>