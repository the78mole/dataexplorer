<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V21.xsd">
    <Device>
        <name>Picolario2</name>
        <manufacturer>Renschler</manufacturer>
        <manufacturerURL>http://www.renschler.de</manufacturerURL>
        <group>logger</group>
        <image>Picolario.jpg</image>
        <usage>false</usage>
    </Device>
    <TimeBase>
        <name>Zeit</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>-1.0</timeStep>
    </TimeBase>
    <DataBlock>
        <format inputType="FILE_IO" formatType="BYTE" size="-64"/>
        <preferredDataLocation></preferredDataLocation>
        <preferredFileExtention>*.p2l</preferredFileExtention>
    </DataBlock>
    <State>
        <property name="Datenaufzeichnung" value="01" type="Integer" description=""/>
    </State>
    <Channels lastUseOrdinal="1">
        <Channel name="Thermik" type="TYPE_CONFIG">
            <Measurement>
                <name>Höhe</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
  				<statistics countTriggerText="Berechnet mittels Steigungstrigger" sumTriggerText="Summe Motorhöhe" sumByTriggerRefOrdinal="3" sigma="false" avg="false" max="true" min="true"/>
                <property name="do_subtract_first" value="true" type="Boolean" description="Reduktion aller gemessenem Werte durch den ersten Messwert"/>
            </Measurement>
            <Measurement>
                <name>Druck</name>
                <symbol>p</symbol>
                <unit>mbar</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
                <property name="factor" value="0.01" type="Double" description="Faktor zu einem gemessenem Wert"/>
            </Measurement>
            <Measurement>
                <name>SpannungRx</name>
                <symbol>Urx</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
                <property name="factor" value="0.0038" type="Double" description="Faktor zu einem gemessenem Wert"/>
            </Measurement>
            <Measurement>
                <name>Steigen</name>
                <symbol>V</symbol>
                <unit>m/s</unit>
                <statistics min="false" max="true" avg="true" sigma="true" countByTrigger="true" countTriggerText="Anzahl gewerteter Steigflüge" sumTriggerTimeText="~ Motorlaufzeit">
                    <trigger level="1000" isGreater="true" minTimeSec="15" comment="Trigger: Steigrate &gt; 1 m/sec &gt; 15 sec"/>
                </statistics>
            </Measurement>
            <Measurement>
                <name>Spannung</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Strom</name>
                <symbol>I</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Kapazität</name>
                <symbol>c</symbol>
                <unit>mAh</unit>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Leistung</name>
                <symbol>P</symbol>
                <unit>W</unit>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Drehzahl</name>
                <symbol></symbol>
                <unit>1/min</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Temperatur</name>
                <symbol></symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Breitengrad</name>
                <symbol>B</symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Längengrad</name>
                <symbol>L</symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Höhe (GPS)</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Speed (GPS)</name>
                <symbol>v</symbol>
                <unit>km/h</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
        </Channel>
        <Channel name="Motor" type="TYPE_CONFIG">
            <Measurement>
                <name>Höhe</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false" sumByTriggerRefOrdinal="5" sumTriggerText="Motorhöhe" comment="Bezogen auf Stromtrigger" ratioRefOrdinal="6" ratioText="Verbrauchte Kapazität/Höhenmeter"/>
                <property name="do_subtract_first" value="true" type="Boolean" description="Reduktion aller gemessenem Werte durch den ersten Messwert"/>
            </Measurement>
            <Measurement>
                <name>Druck</name>
                <symbol>p</symbol>
                <unit>mbar</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false"/>
                <property name="factor" value="0.01" type="Double" description="Faktor zu einem gemessenem Wert"/>
            </Measurement>
            <Measurement>
                <name>SpannungRx</name>
                <symbol>Urx</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
                <property name="factor" value="0.0038" type="Double" description="Faktor zu einem gemessenem Wert"/>
            </Measurement>
            <Measurement>
                <name>Steigen</name>
                <symbol>V</symbol>
                <unit>m/s</unit>
                <statistics min="true" max="true" avg="true" sigma="false" sumTriggerTimeText="~ Motorlaufzeit"/>
                <property name="regression_interval_sec" value="10" type="Integer" description="Zeit in Sekunden über die die regression durchgeführt wird"/>
                <property name="regression_type" value="regression_type_curve" type="String" description="Regressionsrechnungsart"/>
            </Measurement>
            <Measurement>
                <name>Spannung</name>
                <symbol>U</symbol>
                <unit>V</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false" triggerRefOrdinal="5" comment="Bezogen auf Stromtrigger"/>
            </Measurement>
            <Measurement>
                <name>Strom</name>
                <symbol>I</symbol>
                <unit>A</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="false" countByTrigger="true" countTriggerText="Anzahl Steigflüge" sumTriggerTimeText="Motorlaufzeit">
                    <trigger level="3000" isGreater="true" minTimeSec="5" comment="Trigger Motorstrom &gt; 3 A &gt; 5 Sekunden"/>
                </statistics>
            </Measurement>
            <Measurement>
                <name>Kapazität</name>
                <symbol>c</symbol>
                <unit>mAh</unit>
                <statistics min="true" max="true" avg="true" sigma="true" triggerRefOrdinal="5" comment="Bezogen auf Stromtrigger" ratioText="verbrauchte Kapazität/Höhenmeter"/>
            </Measurement>
            <Measurement>
                <name>Leistung</name>
                <symbol>P</symbol>
                <unit>W</unit>
                <statistics min="true" max="true" avg="true" sigma="true" triggerRefOrdinal="5" comment="Bezogen auf Stromtrigger"/>
            </Measurement>
            <Measurement>
                <name>Drehzahl</name>
                <symbol></symbol>
                <unit>1/min</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true" triggerRefOrdinal="5" comment="Bezogen auf Stromtrigger"/>
            </Measurement>
            <Measurement>
                <name>Temperatur</name>
                <symbol></symbol>
                <unit>°C</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="true" sigma="true"/>
            </Measurement>
            <Measurement>
                <name>Breitengrad</name>
                <symbol>B</symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Längengrad</name>
                <symbol>L</symbol>
                <unit>°</unit>
                <active>true</active>
            </Measurement>
            <Measurement>
                <name>Höhe (GPS)</name>
                <symbol>h</symbol>
                <unit>m</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
            <Measurement>
                <name>Speed (GPS)</name>
                <symbol>v</symbol>
                <unit>km/h</unit>
                <active>true</active>
                <statistics min="true" max="true" avg="false" sigma="false"/>
            </Measurement>
        </Channel>
    </Channels>
    <Desktop>
        <property name="table_tab" value="true"/>
        <property name="voltage_per_cell_tab" value="false"/>
        <property name="analog_tab" value="true"/>
        <property name="digital_tab" value="false"/>
        <property name="utility_garphics_tab" value="false" description="UTILITY_GRAPHICS_TAB"/>
        <property name="utility_device_tab" value="false" description="UTILITY_DEVICE_TAB"/>
    </Desktop>
</DeviceProperties>
