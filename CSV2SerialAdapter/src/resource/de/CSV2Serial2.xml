<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V20.xsd">
    <Device>
        <name implementation="CSV2SerialAdapter">CSV2Serial2</name>
        <manufacturer>WB</manufacturer>
        <manufacturerURL>http://www.nongnu.org/dataexplorer</manufacturerURL>
        <group>charger</group>
        <image>CSV2SerialAdapter.png</image>
        <usage>false</usage>
    </Device>
    <SerialPort>
        <port>-</port>
        <baudeRate>115200</baudeRate>
        <dataBits>DATABITS_8</dataBits>
        <stopBits>STOPBITS_1</stopBits>
        <parity>PARITY_NONE</parity>
        <flowControlMode>FLOWCONTROL_NONE</flowControlMode>
        <isRTS>true</isRTS>
        <isDTR>true</isDTR>
        <TimeOut>
            <RTOCharDelayTime>290</RTOCharDelayTime>
            <RTOExtraDelayTime>100</RTOExtraDelayTime>
            <WTOCharDelayTime>0</WTOCharDelayTime>
            <WTOExtraDelayTime>0</WTOExtraDelayTime>
        </TimeOut>
    </SerialPort>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>msec</unit>
        <timeStep>1000.0</timeStep>
    </TimeBase>
    <DataBlock>
        <format inputType="FILE_IO" formatType="VALUE" size="-8"/>
        <format inputType="SERIAL_IO" formatType="BYTE" size="-57"/>
        <timeUnit>msec</timeUnit>
        <separator>semicolon</separator>
        <leader>$</leader>
        <trailer>0D0A</trailer>
    </DataBlock>
    <State>
        <property name="Datenaufnahme" value="01" type="Integer" description=""/>
    </State>
    <Channels>
