<?xml version="1.0" encoding="UTF-8"?><DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V19.xsd">	<Device>		<name>VC820</name>		<manufacturer>Conrad</manufacturer>		<manufacturerURL>http://www.conrad.de</manufacturerURL>		<group>multimeter</group>		<image>VC8X0.jpg</image>		<usage>false</usage>	</Device>	<SerialPort>		<port>-</port>		<baudeRate>2400</baudeRate>		<dataBits>DATABITS_8</dataBits>		<stopBits>STOPBITS_1</stopBits>		<parity>PARITY_NONE</parity>		<flowControlMode>FLOWCONTROL_NONE</flowControlMode>		<isRTS>false</isRTS>		<isDTR>true</isDTR>	</SerialPort>	<TimeBase>		<name>Time</name>		<symbol>t</symbol>		<unit>msec</unit>		<!--time step must be grater than 1000 ms to change cycle time-->		<timeStep>-1.0</timeStep>	</TimeBase>    <DataBlock>        <format inputType="SERIAL_IO" formatType="BYTE" size="14"/>        <trailer>E0</trailer>    </DataBlock>    <Channels>		<Channel type="TYPE_OUTLET" name="Eingang">			<Measurement>				<name>Voltage</name>				<symbol>U</symbol>				<unit>V</unit>				<active>true</active>				<statistics min="true" max="true" avg="true" sigma="true"/>			</Measurement>		</Channel>	</Channels>    <Desktop>        <property value="false" name="table_tab"/>        <property value="true" name="digital_tab"/>        <property value="true" name="analog_tab"/>        <property value="false" name="voltage_per_cell_tab"/>		<property description="UTILITY_GRAPHICS_TAB" value="false" name="utility_garphics_tab"/>		<property description="UTILITY_DEVICE_TAB" value="false" name="utility_device_tab"/>    </Desktop></DeviceProperties>