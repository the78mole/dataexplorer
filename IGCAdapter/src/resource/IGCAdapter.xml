<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:noNamespaceSchemaLocation="DeviceProperties_V44.xsd">
	<Device>
		<name implementation="gde.device.igc.IGCAdapter">IGCAdapter</name>
		<manufacturer>-</manufacturer>
		<manufacturerURL>-</manufacturerURL>
		<group>logger</group>
		<image>IGCAdapter.png</image>
		<usage>false</usage>
	</Device>
	<TimeBase>
		<name>time</name>
		<symbol>t</symbol>
		<unit>sec</unit>
		<timeStep>-1.0</timeStep>
	</TimeBase>
	<DataBlock>
		<format size="-8" formatType="VALUE" inputType="FILE_IO" />
		<timeUnit>sec</timeUnit>
		<separator></separator>
		<leader>B</leader>
		<trailer>0D0A</trailer>
		<preferredFileExtention>*.igc</preferredFileExtention>
	</DataBlock>
	<State>
		<property type="Integer" value="01"
			name="state_flight_recording" />
	</State>
	<Channels>
		<Channel type="TYPE_CONFIG" name="type_universal">
			<Measurement>
				<name>latitude</name>
				<symbol>B</symbol>
				<unit>° '</unit>
				<active>true</active>
			</Measurement>
			<Measurement>
				<name>longitude</name>
				<symbol>L</symbol>
				<unit>° '</unit>
				<active>true</active>
			</Measurement>
			<Measurement>
				<name>altitude (Baro)</name>
				<symbol>h</symbol>
				<unit>m</unit>
				<active>true</active>
				<statistics sigma="false" avg="false" max="true" min="true" />
			</Measurement>
			<Measurement>
				<name>altitude (GPS)</name>
				<symbol>hs</symbol>
				<unit>m</unit>
				<active>true</active>
				<statistics sigma="false" avg="true" max="true" min="true" />
			</Measurement>
			<Measurement>
				<name>climb</name>
				<symbol>vh</symbol>
				<unit>m/s</unit>
				<statistics sigma="false" avg="false" max="true" min="true" />
				<property description="Regression interval in seconds" type="Integer" value="10" name="regression_interval_sec" />
				<property description="Regression arithmetic" type="String" value="regression_type_curve" name="regression_type" />
			</Measurement>
			<Measurement>
				<name>speed</name>
				<symbol>v</symbol>
				<unit>km/h</unit>
				<statistics sigma="false" avg="true" max="true" min="true" />
			</Measurement>
			<Measurement>
				<name>_FXA_</name>
				<symbol></symbol>
				<unit></unit>
				<active>true</active>
				<statistics sigma="false" avg="true" max="true" min="true" />
			</Measurement>
			<Measurement>
				<name>_SIU_</name>
				<symbol></symbol>
				<unit></unit>
				<active>true</active>
				<statistics sigma="false" avg="true" max="true" min="true" />
			</Measurement>
			<Measurement>
				<name>_ENL_</name>
				<symbol></symbol>
				<unit></unit>
				<active>true</active>
				<statistics sigma="false" avg="true" max="true" min="true" />
			</Measurement>
			<Measurement>
				<name>_RPM_</name>
				<symbol></symbol>
				<unit></unit>
				<active>true</active>
				<statistics sigma="false" avg="true" max="true" min="true" />
			</Measurement>
		</Channel>
	</Channels>
	<Desktop>
		<property value="true" name="table_tab" />
		<property value="true" name="digital_tab" />
		<property value="true" name="analog_tab" />
		<property value="false" name="voltage_per_cell_tab" />
		<property description="UTILITY_GRAPHICS_TAB" value="false"
			name="utility_garphics_tab" />
		<property description="UTILITY_DEVICE_TAB" value="false"
			name="utility_device_tab" />
	</Desktop>
</DeviceProperties>
