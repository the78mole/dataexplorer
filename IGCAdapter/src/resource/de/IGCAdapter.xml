<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DeviceProperties xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DeviceProperties_V20.xsd">
    <Device>
        <name implementation="gde.device.igc.IGCAdapter">IGCAdapter</name>
        <manufacturer>-</manufacturer>
        <manufacturerURL>-</manufacturerURL>
        <group>logger</group>
        <image>IGCAdapter.png</image>
        <usage>false</usage>
    </Device>
    <TimeBase>
        <name>Time</name>
        <symbol>t</symbol>
        <unit>sec</unit>
        <timeStep>-1.0</timeStep>
    </TimeBase>
    <DataBlock>
        <format size="4" formatType="VALUE" inputType="FILE_IO"/>
        <timeUnit>sec</timeUnit>
        <separator></separator>
        <leader>B</leader>
        <trailer>0D0A</trailer>
        <preferredFileExtention>*.igc</preferredFileExtention>
    </DataBlock>
    <State>
        <property description="" type="Integer" value="01" name="Flugaufzeichnung"/>
    </State>
    <Channels lastUseOrdinal="0">
        <Channel type="TYPE_CONFIG" name="Standart">
			<Measurement>
				<name>Breitengrad</name>
				<symbol>B</symbol>
				<unit>°</unit>
				<active>true</active>
			</Measurement>
			<Measurement>
				<name>Längengrad</name>
				<symbol>L</symbol>
				<unit>°</unit>
				<active>true</active>
			</Measurement>
			<Measurement>
				<name>Höhe (Baro)</name>
				<symbol>h</symbol>
				<unit>m</unit>
				<active>true</active>
				<statistics sigma="false" avg="false" max="true" min="true"/>
			</Measurement>
            <Measurement>
                <name>Höhe (GPS)</name>
                <symbol></symbol>
                <unit></unit>
                <active>true</active>
                <statistics sigma="false" avg="true" max="true" min="true"/>
            </Measurement>
       </Channel>
    </Channels>
    <Desktop>
        <property value="true" name="table_tab"/>
        <property value="true" name="digital_tab"/>
        <property value="true" name="analog_tab"/>
        <property value="false" name="voltage_per_cell_tab"/>
        <property description="UTILITY_GRAPHICS_TAB" value="false" name="utility_garphics_tab"/>
        <property description="UTILITY_DEVICE_TAB" value="false" name="utility_device_tab"/>
    </Desktop>
</DeviceProperties>
